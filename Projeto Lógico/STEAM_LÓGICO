CONTA (USERNAME, SALDO, LOC_PAIS, LOC_ESTADO, LOC_CIDADE, [USERNAME_REDE])
	USERNAME_REDE -> CONTA_REDE(USERNAME)

AMIGO (USERNAME1, USERNAME2, DATA_AMIZADE)
	USERNAME1 -> CONTA(USERNAME)
	USERNAME2 -> CONTA(USERNAME)

USUARIO(CODIGO)

TEM_ACESSO(USERNAME, CODIGO)
	USERNAME -> CONTA(USERNAME)
	CODIGO-> USUARIO(CODIGO)

EMPRESA(CNPJ, NOME)

APLICATIVO(CODIGO, NOME, PRECO_ATUAL, DATA_LANCAMENTO, CNPJ_DESENVOLVEDOR!, CNPJ_DISTRIBUIDOR!)
	CNPJ_DESENVOLVEDOR-> EMPRESA(CNPJ)
	CNPJ_DISTRIBUIDOR-> EMPRESA(CNPJ)

CATEGORIA(CODIGO, CATEG)
	CODIGO -> APLICATIVO(CODIGO)

JOGO(CODIGO)
	CODIGO -> APLICATIVO(CODIGO)

EXPANSAO(CODIGO_JOGO, CODIGO_EXPANSAO, NOME)
	CÓDIGO_JOGO -> APLICATIVO(CÓDIGO)


SOFTWARE(CODIGO)
	CODIGO -> APLICATIVO(CODIGO)

COMPRA(USERNAME, CODIGO, PRECO_COMPRA, CODIGO_COMPRA, DATA_COMPRA)
	USERNAME -> CONTA(USERNAME)
	CODIGO -> APLICATIVO(CODIGO)

HISTORICO_EXEC(CODIGO_USUARIO, USERNAME_LOGADO, CODIGO_APP, INICIO, FIM)
	CODIGO_USUARIO -> USUARIO(CODIGO)
  USERNAME_LOGADO -> CONTA(USERNAME)
	CODIGO_APP -> APLICATIVO(CODIGO)

CONQUISTA(CÓDIGO_CONQUISTA, NOME, DESCRIÇÃO, RARIDADE, CODIGO_USUARIO, USERNAME_LOGADO, USERNAME_COM_APP, CODIGO_APP, INICIO)
  (CODIGO_USUARIO, USERNAME_LOGADO, USERNAME_COM_APP, CODIGO_APP, INICIO) -> HISTORICO_EXEC(CODIGO_USUARIO, USERNAME_LOGADO, USERNAME_COM_APP, CODIGO_APP, INICIO)


